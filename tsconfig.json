{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "baseUrl": "./src",
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "jsx": "react",
      "paths": {
         "templated-module": [
            "./core"
         ]
      }
   }
}