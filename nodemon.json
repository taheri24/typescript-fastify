{
    "ignore": [
        "**/*.test.ts",
        "**/*.spec.ts",
        ".git",
        "node_modules"
    ],
    "watch":false,
    "env":{
        "TS_NODE_TRANSPILE_ONLY":"true"
    },
    "exec": "ts-node-dev  --transpileOnly  --inspect --ignore-watch  node_modules src/index.ts",
    "ext": "ts"
}